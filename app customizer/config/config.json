{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "footer-editor-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/footerEditor/FooterEditorApplicationCustomizer.js",
          "manifest": "./src/extensions/footerEditor/FooterEditorApplicationCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "sp-init": {
      "path": "https://myefdomain.sharepoint.com/sites/replylabtask3/_layouts/15/init.js",
      "globalName": "$_global_init"
    },
    "microsoft-ajax": {
      "path": "https://myefdomain.sharepoint.com/sites/replylabtask3/_layouts/15/MicrosoftAjax.js",
      "globalName": "Sys",
      "globalDependencies": [
        "sp-init"
      ]
    },
    "sp-runtime": {
      "path": "https://myefdomain.sharepoint.com/sites/replylabtask3/_layouts/15/SP.Runtime.js",
      "globalName": "SP",
      "globalDependencies": [
        "microsoft-ajax"
      ]
    },
    "sharepoint": {
      "path": "https://myefdomain.sharepoint.com/sites/replylabtask3/_layouts/15/SP.js",
      "globalName": "SP",
      "globalDependencies": [
        "sp-runtime"
      ]
    }
  },
  "localizedResources": {
    "FooterEditorApplicationCustomizerStrings": "lib/extensions/footerEditor/loc/{locale}.js"
  }
}